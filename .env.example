# Supabase Configuration
# Get these from https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (Server-side only - NEVER expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Google Gemini API Key
# Get from https://ai.google.dev/
GEMINI_API_KEY=your-gemini-api-key-here

# Casafari API Key
# Get from https://docs.api.casafari.com
CASAFARI_API_KEY=your-casafari-api-key-here

# Stripe Configuration
# Get from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Application URL
# Use http://localhost:3000 for development, your production URL for prod
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Socket.IO Server URL
# For production, use your Render.com Socket.IO server URL
NEXT_PUBLIC_SOCKETIO_URL=https://imoagent-socketio.onrender.com
